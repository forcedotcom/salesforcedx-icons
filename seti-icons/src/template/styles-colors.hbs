/*---------------------
 *  Colors
 *-------------------*/

:root {
	--seti-white: #d4d7d6;
	--seti-red: #cc3e44;
	--seti-blue: #519aba;
	--seti-green: #8dc149;
	--seti-purple: #a074c4;
	--seti-yellow: #cbcb41;
	--seti-grey-light: #6d8086;
	--seti-pink: #f55385;
	--seti-orange: #e37933;
	--seti-ignore: #41535b;
	--seti-grey: #4d5a5e;
}

{{# each colors }}
.{{ ../prefix }}-{{ @key }}:before { color: var({{ this }}) }
{{/ each }}