{
  "name": "@salesforce/seti-icons",
  "version": "0.1.0",
  "description": "The icon font for default VSCode file icons",
  "license": "BSD-3-Clause",
  "scripts": {
    "clean": "node ./scripts/reset.js",
    "svgo": "svgo -f ./src/icons/ --config svgo.config.js",
    "sprite": "node ./scripts/svg-sprite.js --outDir dist --outFile seti-icons.svg",
    "export-to-ts": "node ./scripts/export-to-ts.js  -f ./src/template/mapping.json > ./dist/seti-iconsLibrary.ts",
    "export-to-csv": "node ./scripts/export-to-csv.js -f ./dist/seti-icons.ttf          > ./dist/seti-icons.csv",
    "fonts": "fantasticon",
    "build": "npm run clean && npm run svgo && npm run fonts && npm run export-to-ts && npm run export-to-csv && npm run sprite",
    "release": "release-it",
    "postbuild": "node ./scripts/generate-seti-color.js"
  },
  "repository": "forcedotcom/library-template",
  "bugs": {
    "url": "https://github.com/forcedotcom/library-template/issues"
  },
  "homepage": "https://github.com/forcedotcom/library-template#readme",
  "devDependencies": {
    "ansi-regex": ">=5.0.1",
    "fantasticon": "^1.2.3",
    "opentype.js": "^1.3.3",
    "release-it": "^18.1.2",
    "svg-sprite": "^2.0.4",
    "svgo": "2.8.0",
    "handlebars": "^4.7.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
